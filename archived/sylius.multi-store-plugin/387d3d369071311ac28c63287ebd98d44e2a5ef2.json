{
    "manifests": {
        "sylius/multi-store-plugin": {
            "manifest": {
                "bundles": {
                    "Sylius\\MultiStorePlugin\\SyliusMultiStorePlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "add-lines": [
                    {
                        "file": "assets/admin/entrypoint.js",
                        "content": "import '../../vendor/sylius/multi-store-plugin/assets/admin/entrypoint';",
                        "position": "bottom",
                        "warn_if_missing": true
                    }
                ]
            },
            "files": {
                "config/packages/sylius_multi_store_plugin.yaml": {
                    "contents": [
                        "imports:",
                        "    - { resource: \"@SyliusMultiStorePlugin/config/config.yaml\" }",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/sylius_multi_store_plugin.yaml": {
                    "contents": [
                        "# Remove if you don't use SyliusAdminBundle",
                        "sylius_multi_store_admin:",
                        "    resource: \"@SyliusMultiStorePlugin/config/admin_routing.yaml\"",
                        "    prefix: '/%sylius_admin.path_name%'",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "387d3d369071311ac28c63287ebd98d44e2a5ef2"
        }
    }
}
