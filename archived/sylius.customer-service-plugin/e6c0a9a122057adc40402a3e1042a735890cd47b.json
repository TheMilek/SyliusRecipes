{
    "manifests": {
        "sylius/customer-service-plugin": {
            "manifest": {
                "bundles": {
                    "Sylius\\CustomerServicePlugin\\SyliusCustomerServicePlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "add-lines": [
                    {
                        "file": "assets/admin/entrypoint.js",
                        "content": "import '../../vendor/sylius/customer-service-plugin/assets/admin/entrypoint';",
